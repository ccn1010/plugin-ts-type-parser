{
  "name": "plugin-ts-type-parser",
  "version": "1.0.0",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src",
    "compile": "babel src --out-dir lib",
    "pretest": "npm run lint",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "prepublish": "npm run compile",
    "release": "standard-version",
    "precommit": "lint-staged"
  },
  "homepage": "https://github.com/ccn1010/plugin-ts-type-parser",
  "repository": {
    "type": "git",
    "url": "https://github.com/ccn1010/plugin-ts-type-parser"
  },
  "keywords": [
    "ts",
    "type",
    "parse",
    "convert",
    "deserialize"
  ],
  "author": "Li Yadong <halloling@163.com>",
  "license": "MIT",
  "peerDependencies": {},
  "dependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/helper-plugin-test-runner": "^7.10.4",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "babel-plugin-tester": "^10.0.0",
    "babel-test": "^0.2.4",
    "error-stack-parser": "^2.0.6",
    "escape-string-regexp": "^4.0.0",
    "eslint": "^7.12.1",
    "eslint-cli": "^1.1.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.1",
    "jest-file-snapshot": "^0.5.0",
    "lint-staged": "^10.5.1",
    "strip-ansi": "^6.0.0",
    "@babel/helper-plugin-utils": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "chalk": "^4.1.0"
  },
  "lint-staged": {
    "*.{js}": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "snapshotSerializers": [
      "<rootDir>/test/whitespaceTrimmingSerializer.js"
    ],
    "watchPathIgnorePatterns": [
      "fixtures\\/[^/]+\\/(output|error)\\.js"
    ]
  }
}
